<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="root" maxHeight="800.0" maxWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RootController">
   <center>
      <HBox alignment="CENTER" spacing="400.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="serverChoice" contentDisplay="CENTER" graphicTextGap="0.0" onAction="#ServerMethod" prefHeight="300.0" prefWidth="300.0" text="Server" textAlignment="CENTER" />
            <Button fx:id="clientChoice" onAction="#ClientMethod" prefHeight="300.0" prefWidth="300.0" text="Client" />
         </children>

      </HBox>
   </center>
   <padding>
      <Insets bottom="70.0" left="70.0" right="70.0" top="70.0" />
   </padding>
</BorderPane>
